{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "extends": [
    "eslint:recommended",
    "plugin:@wordpress/eslint-plugin/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-var": 1,
    "react/jsx-curly-spacing": 0,
    "react/jsx-indent": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
